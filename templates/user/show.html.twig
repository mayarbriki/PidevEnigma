{% extends 'base-back.html.twig' %}

{% block title %}User
{% endblock %}

{% block body %}
	<h1>User</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ user.id }}</td>
			</tr>
			<tr>
				<th>Email</th>
				<td>{{ user.email }}</td>
			</tr>
			<tr>
				<th>Roles</th>
				<td>{{ user.roles ? user.roles|json_encode : '' }}</td>
			</tr>
			<tr>
				<th>Password</th>
				<td>{{ user.password }}</td>
			</tr>
			<tr>
				<th>Name</th>
				<td>{{ user.name }}</td>
			</tr>
			<tr>
				<th>Prename</th>
				<td>{{ user.prename }}</td>
			</tr>
			<tr>
				<th>Birthday</th>
				<td>{{ user.birthday ? user.birthday|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>Adress</th>
				<td>{{ user.adress }}</td>
			</tr>
			<tr>
				<th>Phone</th>
				<td>{{ user.phone }}</td>
			</tr>
		</tbody>
	</table>


	{{ include('user/_delete_form.html.twig') }}
{% endblock %}
