{% extends 'base-back.html.twig' %}

{% block title %}Livreur{% endblock %}

{% block body %}
<main id="main" class="main">
    <h1>Livreur</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ livreur.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ livreur.Nom }}</td>
            </tr>
            <tr>
                <th>NumeroTel</th>
                <td>{{ livreur.NumeroTel }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ livreur.Status }}</td>
            </tr>
            <tr>
                <th>HistoireLiv</th>
                <td>{{ livreur.HistoireLiv }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_livreur_index') }}" class="btn btn-primary">back to list</a>

    <a href="{{ path('app_livreur_edit', {'id': livreur.id}) }}" class="btn btn-primary">edit</a>

    {{ include('livreur/_delete_form.html.twig') }}
 </main>
{% endblock %}
