{% extends 'base-back.html.twig' %}

{% block title %}Transport{% endblock %}

{% block body %}
<main id="main" class="main">
    <h1>Transport</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ transport.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ transport.type }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ transport.etat }}</td>
            </tr>
            <tr>
                <th>DelaiLivMoy</th>
                <td>{{ transport.DelaiLivMoy }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_transport_index') }}" class="btn btn-primary">back to list</a>

    <a href="{{ path('app_transport_edit', {'id': transport.id}) }}" class="btn btn-primary">edit</a>

    {{ include('transport/_delete_form.html.twig') }}
 </main>
{% endblock %}
